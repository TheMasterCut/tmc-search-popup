{{#results}}
    <div class="result">

        {{{titleRendered}}}

        {{#hasThumb}}
            <a class="thumb {{thumbPos}}" href="{{url}}">
                <img src="{{thumbUrl}}" alt="">
            </a>
        {{/hasThumb}}

        <div class="excerpt">{{{excerpt}}}</div>

    </div>
{{/results}}
{{^results}}
    <div class="empty-result">
        {{{noResultsText}}}
    </div>
{{/results}}